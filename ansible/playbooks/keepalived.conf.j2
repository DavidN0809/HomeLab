vrrp_instance VI_1 {
    state {{ 'MASTER' if ansible_hostname == 'ubuntu-dl380' else 'BACKUP' }}
    interface enp6s18
    virtual_router_id 51
    priority {{ priority_map[ansible_hostname] }}
    advert_int 1
    authentication {
        auth_type PASS
        auth_pass 1111
    }
    virtual_ipaddress {
        {{ virtual_ip }}
    }
}
